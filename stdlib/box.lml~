
module Box:sig

  type bool = { v: bool }
  type char = { v: char }

  type int8 = { v: int8 }
  type int16 = { v: int16 }
  type int32 = { v: int32 }
  type int64 = { v: int64 }

  type uint8 = { v: uint8 }
  type uint16 = { v: uint16 }
  type uint32 = { v: uint32 }
  type uint64 = { v: uint64 }

  type float = { v: float }
  type double = { v: double }

  val int: int -> t(int)

end = struct
end
